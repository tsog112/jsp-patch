CREATE DATABASE PATCH;
use patch

create table users (
user_id varchar(20) primary key,
password varchar(20),
e_mail varchar(20),
nickname varchar(20)
);

create table pic_memo(
user_id varchar(20),
nickname varchar(20) primary key,
pic varchar(255),
memo varchar(255),
date date,
pic_memo_id int,
title varchar(20)
);

create table contact_admin(
user_id varchar(20),
e_mail varchar(20),
message varchar(255),
contact_id int primary key auto_increment
);

insert into contact_admin (user_id, e_mail, message) values ('121','sample@naver.com', 'hello world!');

alter table contact_admin modify contact_id int not null auto_increment;

desc contact_admin;
show tables;
